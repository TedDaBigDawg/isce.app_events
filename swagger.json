          },
          {
            "name": "obj",
            "in": "body",
            "description": "Event data required to create a new event.",
            "schema": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string",
                  "example": "Party"
                },
                "description": {
                  "type": "string",
                  "example": "Festac Party Event"
                },
                "image": {
                  "type": "string",
                  "example": "image url"
                },
                "location": {
                  "type": "string",
                  "example": "123 Sunny Drive"
                },
                "town": {
                  "type": "string",
                  "example": "Festac"
                },
                "user_id": {
                  "type": "string",
                  "example": "db3a2d6b-b7f8-4832-9600-477af579b293"
                },
                "start_date": {
                  "type": "string",
                  "example": "2024-12-01T18:00:00.000Z"
                },
                "end_date": {
                  "type": "string",
                  "example": "2024-12-02T18:00:00.000Z"
                },
                "gallery": {
                  "type": "string",
                  "example": "array"
                },
                "prices": {
                  "type": "string",
                  "example": "array"
                }
              },
              "required": [
                "title",
                "location",
                "town",
                "user_id",
                "start_date",
                "end_date",
                "gallery",
                "prices"
              ]
            }
