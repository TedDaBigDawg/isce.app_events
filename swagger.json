                "id": {
                  "type": "string",
                  "example": "string"
                },
                "title": {
                  "type": "string",
                  "example": "string"
                },
                "description": {
                  "type": "string",
                  "example": "string"
                },
                "image": {
                  "type": "string",
                  "example": "string"
                },
                "location": {
                  "type": "string",
                  "example": "string"
                },
                "town": {
                  "type": "string",
                  "example": "string"
                },
                "user_id": {
                  "type": "string",
                  "example": "string"
                },
                "start_date": {
